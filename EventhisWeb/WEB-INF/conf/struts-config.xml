    <exception type="java.lang.Throwable" handler="com.webratio.struts.exceptions.GenericExceptionHandler" key="none" path="/WRGenericError.jsp"/>
  </global-exceptions>
  <global-forwards type="org.apache.struts.action.ActionForward">
    <forward name="start" path="/index.jsp"/>
    <forward name="pageUnknown" path="/pageUnknown.jsp"/>
    <forward name="soapFault" path="/soapFault.do"/>
    <forward name="restFault" path="/restFault.do"/>
  </global-forwards>
  <action-mappings type="org.apache.struts.action.ActionMapping">
    <!-- PAGE ACTIONS -->
    <action path="/page_*" type="com.webratio.struts.actions.PageAction" name="{1}FormBean" scope="session" validate="false" parameter="{1}"/>
    <action path="/form_*" type="com.webratio.struts.actions.PageFormAction" name="{1}FormBean" scope="session" validate="true" input="/restore_{1}.do" parameter="{1}"/>
    <action path="/restore_*" type="com.webratio.struts.actions.RestoreContextAction" name="{1}FormBean" scope="session" input="/{1}.do" validate="false" parameter="{1}"/>
    <!-- OPERATION ACTIONS -->
    <action path="/operation_*" type="com.webratio.struts.actions.OperationAction" name="{1}" parameter="{1}"/>
    <action path="/slu1" type="com.webratio.struts.actions.SOAPSolicitAction" parameter="slu1"/>
    <action path="/eru1" type="com.webratio.struts.actions.SOAPErrorResponseAction" parameter="eru1"/>
    <action path="/rsu1" type="com.webratio.struts.actions.SOAPResponseAction" parameter="rsu1"/>
    <!-- WEB SERVICE ACTIONS -->
    <action path="/wsdl" type="com.webratio.struts.actions.WSDLAction" parameter="/wsdl">
      <set-property key="elements" value="wsv1|port1"/>
    </action>
    <action path="/soapFault" type="com.webratio.struts.actions.SOAPFaultAction" parameter="/soapFault" name="soapFault"/>
    <action path="/restFault" type="com.webratio.struts.actions.RESTFaultAction" parameter="/restFault" name="restFault"/>
    <action path="/EventosService/wsdl" type="com.webratio.struts.actions.WSDLAction" parameter="/wsdl" scope="session" validate="false">
      <set-property key="elements" value="wsv1|port1"/>
    </action>
    <action path="/EventosService/mostraeventos/Area" type="com.webratio.struts.actions.RESTSolicitAction" parameter="slu1">
      <forward name="genericError" path="/restFault.do"/>
      <forward path="/xou1.do" name="success"/>
    </action>
    <!-- OTHER ACTIONS -->
    <action path="/luceneIndexer" type="com.webratio.struts.actions.LuceneIndexerAction" parameter="/luceneIndexer"/>
    <action name="loginFormBean" path="/prepareLogin" type="com.webratio.struts.actions.PrepareLoginAction"/>
    <action name="loginFormBean" path="/loginAction" type="com.webratio.struts.actions.LoginAction"/>
  </action-mappings>
  <message-resources factory="com.webratio.struts.WRPropertyMessageResourcesFactory" parameter="ApplicationResources|en|US" null="false"/>
</struts-config>
